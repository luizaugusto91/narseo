  inherited qConsulta: TUniQuery
    SQL.Strings = (
      'SELECT codigo, '
      #9'empresa, '
      #9'cfop, '
      #9'destinatario, '
      #9'remetente, '
      #9'origem, '
      #9'destino, '
      #9'tipo_nota, '
      #9'frota'
      '   FROM cte_parametros'
      '  WHERE codigo = :codigo and empresa = :empresa;')
    ParamData = <
      item
        DataType = ftInteger
        Name = 'codigo'
        ParamType = ptInput
        Value = 0
      end
      item
        DataType = ftInteger
        Name = 'empresa'
        ParamType = ptInput
        Value = 0
      end>
    object FieldConsultacodigo: TIntegerField
      FieldName = 'codigo'
    end
    object FieldConsultaempresa: TIntegerField
      FieldName = 'empresa'
      Required = True
    end
    object FieldConsultacfop: TIntegerField
      FieldName = 'cfop'
    end
    object FieldConsultadestinatario: TIntegerField
      FieldName = 'destinatario'
    end
